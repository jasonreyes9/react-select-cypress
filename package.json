{
  "name": "react-select",
  "version": "0.4.2",
  "description": "A Select control built with and for ReactJS",
  "main": "lib/Select.js",
  "author": "Jed Watson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JedWatson/react-select.git"
  },
  "dependencies": {
    "classnames": "^1.2.0",
    "lodash": "^3.5.0",
    "react": ">=0.12.0",
    "react-input-autosize": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^2.0.2",
    "eslint": "^0.17.1",
    "eslint-plugin-react": "^1.6.1",
    "gulp": "^3.8.11",
    "jest-cli": "^0.4.0",
    "react-component-gulp-tasks": "^0.4.2",
    "react-tools": "^0.13.1"
  },
  "browserify-shim": {
    "classnames": "global:classNames",
    "react": "global:React",
    "react-input-autosize": "global:AutosizeInput",
    "lodash": "global:_"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint ./; true"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testFileExtensions": [
      "js"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "select",
    "multiselect",
    "combobox",
    "input",
    "form",
    "ui"
  ]
}
